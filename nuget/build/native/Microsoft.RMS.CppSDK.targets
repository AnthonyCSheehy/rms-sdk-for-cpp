<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemDefinitionGroup Label="Debug" Condition="'$(Platform.ToLower())' == 'win32' And ( $(Configuration.ToLower().IndexOf('debug')) &gt; -1)">
    <Link>
      <AdditionalDependencies>$(MSBuildThisFileDirectory)../../lib/Debug/x86/rmsd.lib;%(AdditionalDependencies)</AdditionalDependencies>
	  <AdditionalDependencies>$(MSBuildThisFileDirectory)../../lib/Debug/x86/rmscryptod.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Label="Release" Condition="'$(Platform.ToLower())' == 'win32' And ( $(Configuration.ToLower().IndexOf('debug')) == -1 )">
    <Link>
      <AdditionalDependencies>$(MSBuildThisFileDirectory)../../lib/Release/x86/rms.lib;%(AdditionalDependencies)</AdditionalDependencies>
	  <AdditionalDependencies>$(MSBuildThisFileDirectory)../../lib/Release/x86/rmscrypto.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Label="Debug" Condition="'$(Platform.ToLower())' == 'x64' And ( $(Configuration.ToLower().IndexOf('debug')) &gt; -1)">
    <Link>
      <AdditionalDependencies>$(MSBuildThisFileDirectory)../../lib/Debug/amd64/rmsd.lib;%(AdditionalDependencies)</AdditionalDependencies>
	  <AdditionalDependencies>$(MSBuildThisFileDirectory)../../lib/Debug/amd64/rmscryptod.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Label="Release" Condition="'$(Platform.ToLower())' == 'x64' And ( $(Configuration.ToLower().IndexOf('debug')) == -1 )">
    <Link>
      <AdditionalDependencies>$(MSBuildThisFileDirectory)../../lib/Release/amd64/rms.lib;%(AdditionalDependencies)</AdditionalDependencies>
	  <AdditionalDependencies>$(MSBuildThisFileDirectory)../../lib/Release/amd64/rmscrypto.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
</Project>